\par   \plain\f1\fs18\cf3\b try\plain\f1\fs18\cf3 
\par     ListBox1.Clear;
\par     Enum.\plain\f1\fs18\cf1 \protect <HS 160>\plain\f1\fs18\cf3 Init\plain\f1\fs18\cf1 \protect </HS 160>\plain\f1\fs18\cf3 ;
\par     \plain\f1\fs18\cf3\b while\plain\f1\fs18\cf3  \plain\f1\fs18\cf3\b not\plain\f1\fs18\cf3  Enum.\plain\f1\fs18\cf1 \protect <HS 161>\plain\f1\fs18\cf3 AtEnd\plain\f1\fs18\cf1 \protect </HS 161>\plain\f1\fs18\cf3  \plain\f1\fs18\cf3\b do\plain\f1\fs18\cf3 
\par       ListBox1.Items.Add(\plain\f1\fs18\cf1 \protect <HS 163>\plain\f1\fs18\cf3 SpecialFolderIdToStr\plain\f1\fs18\cf1 \protect </HS 163>\plain\f1\fs18\cf3 (Enum.\plain\f1\fs18\cf1 \protect <HS 162>\plain\f1\fs18\cf3 Next\plain\f1\fs18\cf1 \protect </HS 162>\plain\f1\fs18\cf3 ));
\par   \plain\f1\fs18\cf3\b finally\plain\f1\fs18\cf3 
\par     Enum.Free;
\par   \plain\f1\fs18\cf3\b end\plain\f1\fs18\cf3 ;
\par \plain\f1\fs18\cf3\b end\plain\f1\fs18\cf3 ;
\par 
